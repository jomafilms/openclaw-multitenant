# Copy to .env and fill in your values

# Twilio (for WhatsApp)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# Deployment hosts (used by scripts/deploy.sh)
DEPLOY_MGMT_HOST=root@your-management-server-ip
DEPLOY_UI_HOST=root@your-ui-server-ip
DEPLOY_AGENT_HOST=root@your-agent-server-ip
